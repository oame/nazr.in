{
  "name": "nazrin-api",
  "description": "nazr.in API",
  "version": "0.0.0",
  "scripts": {
    "build": "shx rm -rf dist && tsc",
    "dev": "npm-run-all build --parallel dev:*",
    "dev:server": "PORT=5000 nodemon ./dist/server.js",
    "dev:tsc": "tsc -w",
    "production:postinstall": "cross-env NODE_ENV=production yarn build",
    "production:server": "node ./dist/server",
    "test": "yarn build && nyc ava",
    "test:coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "dependencies": {
    "base62": "^2.0.0",
    "body-parser": "^1.19.0",
    "corser": "^2.0.1",
    "express": "^4.17.1",
    "mongoose": "^5.9.17",
    "mongoose-sequence": "^5.2.2",
    "morgan": "^1.10.0",
    "node-fetch": "^3.1.1",
    "raven": "^2.4.2",
    "validator": "^13.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.6",
    "@types/mongoose": "^5.7.21",
    "@types/mongoose-sequence": "^3.0.4",
    "@types/morgan": "^1.9.0",
    "@types/node": "^14.0.9",
    "ava": "^3.8.2",
    "coveralls": "^3.1.0",
    "mongodb-memory-server": "^6.6.1",
    "nyc": "^15.1.0",
    "supertest": "^4.0.2"
  },
  "main": "./dist/app.js",
  "license": "MIT"
}
